            android:textSize="20sp"
            tools:layout_editor_absoluteX="90dp"
            tools:layout_editor_absoluteY="641dp" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="31dp"
            android:layout_height="46dp"
            android:tint="#2167F3"
            app:srcCompat="@drawable/calendar"
            tools:layout_editor_absoluteX="370dp"
            tools:layout_editor_absoluteY="293dp" />

        <EditText
            android:id="@+id/DeliveryD_add"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="date"
            tools:layout_editor_absoluteX="116dp"
            tools:layout_editor_absoluteY="158dp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/TxtAddDate"
            android:textSize="16sp"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="320dp" />

        <EditText
            android:id="@+id/description_add"
            android:layout_width="254dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:gravity="start|top"
            android:inputType="textMultiLine"
            tools:layout_editor_absoluteX="116dp"
            tools:layout_editor_absoluteY="294dp" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/TxtAddDescription"
            android:textSize="16sp"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="251dp" />

        <EditText
            android:id="@+id/subTitle_add"
            android:layout_width="254dp"
            android:layout_height="50dp"
            android:ems="10"
            android:inputType="text"
            tools:layout_editor_absoluteX="116dp"
            tools:layout_editor_absoluteY="225dp" />

        <EditText
            android:id="@+id/Title_add"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
