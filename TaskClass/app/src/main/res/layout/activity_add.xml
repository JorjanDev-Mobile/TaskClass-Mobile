            tools:layout_editor_absoluteY="369dp" />

        <RadioButton
            android:id="@+id/radioButton14"
            android:layout_width="30dp"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="323dp"
            tools:layout_editor_absoluteY="467dp" />

        <ImageView
            android:id="@+id/btnAttach_add"
            android:layout_width="26dp"
            android:layout_height="28dp"
            android:tint="#2167F3"
            app:srcCompat="@drawable/attach"
            tools:layout_editor_absoluteX="300dp"
            tools:layout_editor_absoluteY="369dp" />

        <RadioButton
            android:id="@+id/radioButton15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="323dp"
            tools:layout_editor_absoluteY="445dp" />

        <CheckBox
            android:id="@+id/Reminder_add"
            android:layout_width="109dp"
            android:layout_height="28dp"
            android:text="@string/btnReminder"
            tools:layout_editor_absoluteX="275dp"
            tools:layout_editor_absoluteY="558dp" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="77dp"
            android:layout_height="33dp"
            android:text="@string/btnCourse"
            tools:layout_editor_absoluteX="60dp"
            tools:layout_editor_absoluteY="558dp" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="36dp"
            android:layout_height="25dp"
            android:tint="#2167F3"
            app:srcCompat="@drawable/course"
            tools:layout_editor_absoluteX="24dp"
            tools:layout_editor_absoluteY="558dp" />

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="417dp"
            android:layout_height="76dp"
            android:background="?attr/colorPrimary"
            android:backgroundTint="#2167F3"
            android:layerType="none"
            android:minHeight="?attr/actionBarSize"
            android:textAlignment="textStart"
            android:textDirection="anyRtl"
            android:theme="?attr/actionBarTheme"
            app:title="@string/TxtTitle"
            app:titleMarginStart="160dp"
            app:titleTextAppearance="@style/TextAppearance.AppCompat.Large"
            app:titleTextColor="#FFF8F8"
            tools:layout_editor_absoluteX="-3dp"
            tools:layout_editor_absoluteY="-3dp" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="42dp"
            android:layout_height="33dp"
            android:tint="#FFFFFF"
            app:srcCompat="@drawable/return_page"
            tools:layout_editor_absoluteX="18dp"
            tools:layout_editor_absoluteY="25dp" />

        <androidx.constraintlayout.helper.widget.Layer android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <androidx.constraintlayout.helper.widget.Layer android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <androidx.constraintlayout.helper.widget.Layer android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <androidx.constraintlayout.helper.widget.Layer android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>